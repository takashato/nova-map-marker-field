{
    "name": "takashato/nova-map-marker-field",
    "description": "A package for adding a map marker using Mapbox to Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "matanyadaev/laravel-eloquent-spatial": "^2.10.1"
    },
    "autoload": {
        "psr-4": {
            "Takashato\\NovaMapMarkerField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Takashato\\NovaMapMarkerField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
